add_compile_flags:
  - --no-cuda-version-check
diagrams:
  t20051_sequence:
    type: sequence
    glob:
      - t20051.cu
    include:
      namespaces:
        - clanguml::t20051
    exclude:
      callee_types:
        - cuda_device
    using_namespace: clanguml::t20051
    from:
      - function: "clanguml::t20051::tmain()"